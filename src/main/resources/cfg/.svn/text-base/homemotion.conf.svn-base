[Notification-Service]
EventCollector.severity=INFO
# NotificationDecorator=yxa

[modes]
defaultMode=AUTO
updateStartupDefer=3000
updateInterval=500

[device-adapter]
org.homemotion.devices.ActorDevice
org.homemotion.devices.ClimateControlDevice
org.homemotion.devices.SensorDevice
org.homemotion.security.SecurityDevice

[trigger-strategies]
org.homemotion.trigger.impl.CRON
org.homemotion.trigger.impl.Observer
org.homemotion.trigger.impl.Periodic
org.homemotion.trigger.impl.Scheduled
org.homemotion.trigger.impl.Scriptable

[calendar-types]
org.homemotion.calendar.DailyCalendar
org.homemotion.calendar.WeeklyCalendar
org.homemotion.calendar.MonthlyCalendar
org.homemotion.calendar.AnnualCalendar
org.homemotion.calendar.HolidayCalendar
org.homemotion.calendar.CombinedCalendar
