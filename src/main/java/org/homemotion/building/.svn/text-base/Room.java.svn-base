package org.homemotion.building;

import org.homemotion.dao.spi.AbstractConfiguredItem;

public class Room extends AbstractConfiguredItem {

	private static final long serialVersionUID = 8244699449321737136L;

	private Floor floor;
	
	public Room(String id, String binding) {
		super(id, binding);
	}

	public final void setFloor(Floor floor) {
		this.floor = floor;
	}

	public final Floor getFloor() {
		return floor;
	}

}
